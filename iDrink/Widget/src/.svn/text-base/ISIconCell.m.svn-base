#import "ISIconCell.h"

@implementation ISIconCell
@synthesize iconView = _iconView;
@synthesize label = _label;

-(void)populateWithIcon:(UIImage*)image withText:(NSString*)text {
	[_iconView setImage:image];
	[_label setText:text];
}
@end
